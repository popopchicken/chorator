            <script>
                var myDataRef = new Firebase('https://incandescent-torch-9651.firebaseio.com/');
                $('#nameInput').keypress(function (e) {
                  if (e.keyCode == 13) {
                    var tname = $('#taskName').val();
                    var name = $('#nameInput').val();
                    myDataRef.push({task_name: tname, name_1: name});
                    $('#nameInput').val('');
                  }
                });
                myDataRef.on('child_added', function(snapshot) {
                  var message = snapshot.val();
                  displayTask(message.task_name, message.name_1);
                });
                function displayTask(name, text) {
                  $('<div/>').text(text).prepend($('<em/>').text(name+': ')).appendTo($('#messagesDiv'));
                  $('#messagesDiv')[0].scrollTop = $('#messagesDiv')[0].scrollHeight;
                };
            </script> 

